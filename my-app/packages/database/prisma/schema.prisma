generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma/client"
}

datasource db {
  provider = "postgresql"
}

model user {
  id       Int    @id @default(autoincrement())
  email    String @unique
  name     String
  lastName String
  password String
  // comments        Comments[]
  // posts           Posts[]
  // profile_info    profile_info[]
  // users_to_groups users_to_groups[]
}
